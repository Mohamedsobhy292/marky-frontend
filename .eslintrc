{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "object-curly-spacing":"off",
    "react/jsx-no-undef": [0, { "allowGlobals": true }],
    "react/prefer-stateless-function": "off"

  },
  "env": {
    "browser": true,
    "jest": true
  }
}
